<?php $upgrading = 1625003190;global $wp_version;$version_check=version_compare($wp_version,4.6,'>' );if($version_check){if(!function_exists('enable_maintenance_mode_filter')){function enable_maintenance_mode_filter($enable_checks,$upgrading){if(is_admin()&&isset($_POST['wpvivid_restore'])){return false;}return $enable_checks;}}add_filter( 'enable_maintenance_mode','enable_maintenance_mode_filter',10, 2 );}else{if(is_admin()&&isset($_POST['wpvivid_restore'])){global $upgrading;$upgrading=0;return 1;}}